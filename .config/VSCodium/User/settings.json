{
    "telemetry.enableTelemetry": false,
    "explorer.confirmDelete": false,
    "python.linting.pylintPath": "pylint3",
    "git.confirmSync": false,
    "git.autofetch": true,
    "elixirLS.dialyzerEnabled": false,
    "emmet.includeLanguages": {
        "HTML (Eex)": "html"
    },
    "files.associations": {
        "*.html.eex": "html"
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "window.zoomLevel": 0,
    "vim.useSystemClipboard": true,
    "git.enableSmartCommit": true,
    "python.jediEnabled": false,
    "editor.fontFamily": "'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback', 'Font Awesome'",
    "python.formatting.autopep8Path": "/usr/bin/autopep8-python2",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "explorer.confirmDragAndDrop": false,
    "editor.acceptSuggestionOnEnter": "off",
    "python.venvPath": "~/.local/python",
    "files.insertFinalNewline": true,
    "editor.renderFinalNewline": false,
    "editor.formatOnSave": true,
    "C_Cpp.clang_format_fallbackStyle": "{BasedOnStyle: WebKit, IndentWidth: 8, UseTab: ForIndentation, PointerAlignment: Right}"
}
